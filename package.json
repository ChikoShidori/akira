{
	"name": "@chikoshidori/akira",
	"version": "0.0.1",
	"description": "template",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "@chikoshidori",
	"license": "MIT",
	"scripts": {
		"lint": "eslint src scripts --ext ts,mjs --fix",
		"format": "prettier --write --loglevel=warn {src,scripts}/**/*.ts",
		"docs": "typedoc",
		"update": "yarn upgrade-interactive",
		"build": "tsc -b src && rollup -c scripts/rollup.bundle.ts && gen-esm-wrapper dist/index.js dist/index.mjs",
		"sversion": "standard-version",
		"commit": "git-cz",
		"prepublishOnly": "rollup-type-bundler -v -e url events",
		"prepare": "husky install .github/husky",
		"test": "yarn build && node test/test.mjs"
	},
	"dependencies": {
		"@sapphire/utilities": "^3.3.0",
		"playwright": "^1.19.2",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.1",
		"@commitlint/config-conventional": "^16.2.1",
		"@favware/npm-deprecate": "^1.0.4",
		"@favware/rollup-type-bundler": "^1.0.7",
		"@sapphire/eslint-config": "^4.2.2",
		"@sapphire/prettier-config": "^1.3.0",
		"@sapphire/ts-config": "^3.3.1",
		"@types/node": "^17.0.14",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "~8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.5",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"rollup": "^2.70.0",
		"rollup-plugin-version-injector": "^1.3.3",
		"standard-version": "^9.3.2",
		"ts-node": "^10.7.0",
		"typedoc": "^0.22.13",
		"typedoc-plugin-mdn-links": "^1.0.5",
		"typescript": "^4.6.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/chikoshidori/akira.git"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"ts",
		"yarn",
		"anime",
		"scraper",
		"typescript",
		"chikoshidori",
		"@chikoshidori/akira"
	],
	"bugs": {
		"url": "https://github.com/chikoshidori/akira/issues"
	},
	"homepage": "https://chikum.me/akira",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.2.0"
}
